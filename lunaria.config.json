{
	"$schema": "./node_modules/@lunariajs/core/config.schema.json",
	"repository": {
		"name": "wangqiyangx/wangqiyangX.github.io"
	},
	"files": [
		{
			"location": "**/config.ts",
			"pattern": "@lang/@path",
			"type": "universal"
		},
		{
			"location": "**/*.md",
			"pattern": "@lang/@path",
			"type": "universal"
		}
	],
	"defaultLocale": {
		"label": "简体中文",
		"lang": "zh"
	},
	"locales": [
		{
			"label": "English",
			"lang": "en"
		}
	],
	"outDir": ".vitepress/dist/_translations",
	"ignoreKeywords": ["lunaria-ignore"]
}
